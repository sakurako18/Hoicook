  <div class="container">
    <!--検索できなかった場合のボタン-->
    <!--< % if @tags %>-->
    <!--< %= @tags %>-->
    <!--< %= link_to "もう一度検索する", user_recipes_path, class:"navbar-brand" %>-->
    <!--< % else %>-->
    
    <p><h5>月齢</h5></p>
      <%= form_with model: @recipe, url: recipes_path, method: :get do |f| %>
        <% Tag.all.each do |t| %>
          <%= check_box :tag_ids, t.tag_name %> <%= t.tag_name %><br>
        <% end %>

    <p><h5>ジャンル検索</h5></p>

        <%= f.collection_check_boxes(:genre_ids, @genres, :id, :genre_name) do |b| %>
          <%= b.label { b.check_box + b.text } %>
        <% end %>

        <p><%= f.submit '検索' %></p>
      <% end %>
  </div>